{
    "name": "Chikusei",
    "phase": "train", // train or val
    "gpu_ids": [
        1
    ],
    "path": { //set the path
        "log": "logs",
        "tb_logger": "tb_logger",
        "results": "results",
        "mat_results": "mat_results",
        "checkpoint": "checkpoint",
        "resume_state": null // for validation, modify it to "./experiments/ddpm/*/checkpoints/I***_E***"
    },
    "datasets": {
        "train": {
            "data_len": -1 // -1 represents all data used in train
        },
        "val": {
            "data_len": 50
        }
    },
    "model": {
        "which_model_G": "ddpm",
        "finetune_norm": false,
        "unet": {
            "in_channel": 5,
            "out_channel": 5,
            "inner_channel": 64,
            "channel_multiplier": [
                1,
                2,
                4,
                8,
                8
            ],
            "attn_res": [
                16
            ],
            "res_blocks": 2,
            "dropout": 0.2
        },
        "beta_schedule": { // use munual beta_schedule for acceleration
            "train": {
                "schedule": "linear",
                "n_timestep": 2000,
                "linear_start": 1e-6,
                "linear_end": 1e-2
            },
            "val": {
                "schedule": "linear",
                "n_timestep": 2000,
                "linear_start": 1e-6,
                "linear_end": 1e-2
            }
        },
        "diffusion": {
            "image_size": 256,
            "channels": 5, //sample channel
            "conditional": false // unconditional generation or unconditional generation(super_resolution)
        }
    },
    "train": {
        "n_iter": 30000000,
        "val_freq": 1e4,
        "save_checkpoint_freq": 1e4,
        "print_freq": 200,
        "optimizer": {
            "type": "adam",
            "lr": 1e-4
        },
        "ema_scheduler": { // not used now
            "step_start_ema": 5000,
            "update_ema_every": 1,
            "ema_decay": 0.9999
        }
    },
    "wandb": {
        "project": "MSF-Diff"
    }
}
